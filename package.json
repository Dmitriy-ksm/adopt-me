    "webpack",
    "babel",
    "eslin"
  ],
  "author": "DmitryMashtaler",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Dmitriy-ksm/adopt-me/issues"
  },
  "homepage": "https://github.com/Dmitriy-ksm/adopt-me#readme",
