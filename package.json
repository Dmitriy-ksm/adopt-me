  "private": true,
  "description": "Some homework for the course http://bit.ly/react-6",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "start": "webpack-dev-server --history-api-fallback --config webpack.dev.js --open",
    "build:client": "webpack --config webpack.prod.client.js",
    "build:server": "webpack --config webpack.prod.server.js",
    "build": "npm run build:client && npm run build:server",
    "SSR": "npm -s run build && node dist-server/server.js",
    "startSSR": "node dist-server/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dmitriy-ksm/adopt-me.git"
  },
